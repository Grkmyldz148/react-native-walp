{
    "presets": [
      "module:metro-react-native-babel-preset"
    ],
    "plugins": [
      [
        "module-resolver",
        {
          "root": [
            "./app"
          ],
          "extensions": [
            ".js",
            ".json"
          ],
          "alias": {
            "navigation": "./app/navigation",
            "components": "./app/components",
            "_constants": "./app/constants",
            "elements": "./app/elements",
            "utils": "./app/utils",
            "pages": "./app/pages"
          }
        }
      ]
    ]
  }